{
  "name": "cedar-mastra-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- concurrently -n \"NEXT,MASTRA\" -c \"blue.bold,magenta.bold\" \"npm run dev:next\" \"npm run dev:mastra\"",
    "dev:next": "next dev --turbopack",
    "dev:mastra": "npm --prefix src/backend run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "pretty": "npx prettier --write ."
  },
  "dependencies": {
    "@xyflow/react": "^12.8.2",
    "cedar-os": "^0.0.8",
    "lucide-react": "^0.532.0",
    "motion-plus-react": "^1.5.1",
    "next": "15.4.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.4",
    "prettier": "^3.2.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
